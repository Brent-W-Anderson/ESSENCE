{
    "name": "frontend",
    "type": "module",
    "private": true,
    "scripts": {
        "start": "yarn build:css && npx concurrently \"yarn vite\" \"yarn generate:scss-types\"",
        "build": "vite build && yarn build:css",
        "build:css": "sass --no-source-map \"src/index.scss\" \"dist/assets/css/index.css\"",
        "generate:scss-types": "typed-scss-modules \"src/**/*.module.scss\" --exportType default --nameFormat none -w"
    },
    "dependencies": {
        "@solidjs/router": "^0.13.5",
        "ammojs3": "^0.0.11",
        "sass": "^1.77.5",
        "solid-js": "^1.8.17",
        "three": "^0.165.0",
        "vite": "^5.3.1"
    },
    "devDependencies": {
        "@types/node": "^20.14.2",
        "@types/three": "^0.165.0",
        "empty-module": "^0.0.2",
        "postcss": "^8.4.38",
        "rollup-plugin-visualizer": "^5.12.0",
        "typed-scss-modules": "^8.0.1",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-solid": "^2.10.2",
        "vite-plugin-static-copy": "^1.0.5"
    },
    "resolutions": {
        "braces": "^3.0.3"
    },
    "packageManager": "yarn@4.3.0"
}
