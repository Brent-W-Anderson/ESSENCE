{
    "name": "essence",
    "type": "module",
    "scripts": {
        "start": "concurrently \"yarn vite\" \"yarn generate:scss-types\"",
        "build": "vite build",
        "lint": "concurrently \"yarn lint:js\" \"yarn lint:styles\"",
        "lint:fix": "concurrently \"yarn lint:js:fix\" \"yarn lint:styles:fix\"",
        "lint:js": "eslint --config .eslintrc.config.cjs",
        "lint:js:fix": "eslint --fix --config .eslintrc.config.cjs",
        "lint:styles": "stylelint \"**/*.scss\"",
        "lint:styles:fix": "stylelint \"**/*.scss\" --fix",
        "generate:scss-types": "typed-scss-modules src --exportType default --nameFormat none -w"
    },
    "devDependencies": {
        "@types/three": "^0.165.0",
        "@typescript-eslint/eslint-plugin": "^7.13.0",
        "@typescript-eslint/parser": "^7.13.0",
        "concurrently": "^8.2.2",
        "empty-module": "^0.0.2",
        "eslint": "^9.4.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-solid": "^0.14.0",
        "postcss": "^8.4.38",
        "prettier": "^3.3.2",
        "rollup-plugin-visualizer": "^5.12.0",
        "stylelint": "^16.6.1",
        "stylelint-config-standard-scss": "^13.1.0",
        "typed-scss-modules": "^8.0.1",
        "vite-plugin-compression": "^0.5.1",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-solid": "^2.10.2"
    },
    "dependencies": {
        "@solidjs/router": "^0.13.5",
        "sass": "^1.77.5",
        "solid-js": "^1.8.17",
        "three": "^0.165.0",
        "typescript": "^5.4.5",
        "vite": "^5.2.13"
    },
    "packageManager": "yarn@4.2.2"
}
