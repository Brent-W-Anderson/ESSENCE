{
    "name": "solid-vite-boilerplate",
    "type": "module",
    "scripts": {
        "setup": "yarn dlx @yarnpkg/sdks vscode",
        "start": "concurrently \"yarn vite\" \"yarn generate:scss-types\"",
        "build": "vite build",
        "lint": "yarn lint:js && yarn lint:styles",
        "lint:fix": "yarn lint:js:fix && yarn lint:styles:fix",
        "lint:js": "eslint --config .eslintrc.config.cjs",
        "lint:js:fix": "eslint --fix --config .eslintrc.config.cjs",
        "lint:styles": "stylelint \"**/*.scss\"",
        "lint:styles:fix": "stylelint \"**/*.scss\" --fix",
        "generate:scss-types": "typed-scss-modules src --exportType default --nameFormat none -w"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^7.8.0",
        "@typescript-eslint/parser": "^7.8.0",
        "concurrently": "^8.2.2",
        "eslint": "^9.1.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-solid": "^0.14.0",
        "prettier": "^3.2.5",
        "stylelint": "^16.4.0",
        "stylelint-config-prettier": "^9.0.5",
        "stylelint-config-standard": "^36.0.0",
        "typed-scss-modules": "^8.0.1",
        "vite-plugin-html": "^3.2.2",
        "vite-plugin-solid": "^2.10.2"
    },
    "dependencies": {
        "@solidjs/router": "^0.13.2",
        "sass": "^1.76.0",
        "solid-js": "^1.8.17",
        "typescript": "^5.4.5",
        "vite": "^5.2.10"
    },
    "packageManager": "yarn@4.1.1"
}
